[
  {
    "key": "dwkit-application-header",
    "data-buildertype": "container",
    "style-customcss": "dwkit-application-header",
    "children": [
      {
        "key": "dwkit-application-header-left",
        "data-buildertype": "container",
        "style-float": "left",
        "children": [
          {
            "key": "logo",
            "data-buildertype": "image",
            "src": "/images/logo.svg",
            "style-height": "",
            "style-width": "",
            "href": "/",
            "style-customcss": "dwkit-application-header-logo"
          }
        ],
        "style-marginLeft": "",
        "style-customcss": "dwkit-application-header-left"
      },
      {
        "key": "dwkit-application-header-right",
        "data-buildertype": "container",
        "children": [
          {
            "key": "button_2",
            "data-buildertype": "button",
            "content": "Create a vacation request",
            "secondary": true,
            "events": {
              "onClick": {
                "active": true,
                "actions": [
                  "redirect"
                ],
                "targets": [],
                "parameters": [
                  {
                    "name": "target",
                    "value": "/form/DocumentEdit"
                  }
                ]
              }
            }
          },
          {
            "key": "button_1",
            "data-buildertype": "button",
            "content": "Settings",
            "secondary": true,
            "events": {
              "onClick": {
                "active": true,
                "actions": [
                  "redirect"
                ],
                "targets": [],
                "parameters": [
                  {
                    "name": "target",
                    "value": "/form/settings"
                  }
                ]
              }
            }
          },
          {
            "key": "currentUser",
            "data-buildertype": "dropdowntrigger",
            "defaultValue": "User",
            "items": [
              {
                "target": "/admin",
                "title": "Admin panel"
              },
              {
                "target": "/form/settings",
                "title": "Settings"
              },
              {
                "target": "/account/logoff",
                "title": "Logout"
              }
            ],
            "events": {
              "onItemClick": {
                "active": true,
                "actions": [
                  "redirect"
                ],
                "targets": [],
                "parameters": []
              }
            },
            "imageUrl": "/images/unknown.png"
          }
        ],
        "style-float": "right",
        "style-customcss": "dwkit-application-header-right"
      }
    ]
  }
]